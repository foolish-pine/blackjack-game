{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/recommended-requiring-type-checking",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "plugins": [],
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-alert": "warn",
		"no-console": "warn",
		"no-unused-vars": "warn",
		"no-var": "warn",
		"eqeqeq": "warn"
  }
}